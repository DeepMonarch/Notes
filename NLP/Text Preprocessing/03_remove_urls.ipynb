   "id": "4e0a7c5d",
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "def remove_urls(text):\n",
    "    \"\"\"Remove urls from a string\"\"\"\n",
    "    url_pattern = re.compile(r'http\\S+|www\\S+')\n",
    "    return url_pattern.sub(r'', text)"
   ]
  },
