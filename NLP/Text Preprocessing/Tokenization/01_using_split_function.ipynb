   ],
   "source": [
    "# word tokenization\n",
    "sent1 = 'I am going to delhi!' # Limitation: delhi! is considered as one word\n",
    "print(sent1.split())\n",
    "\n",
    "# sentence tokenization\n",
    "sent2 = 'I am going to delhi. I will meet my friends there.'\n",
    "print(sent2.split('. '))"
   ]
  }
